package vnx;

class PlayerStatus {
	
	string filename;
	
	long begin_time;			// [usec]
	long end_time;				// [usec]
	long current_time;			// [usec]
	
	double progress;			// 0 to 1 [1]
	
	double actual_speed;		// [1]
	
	double bandwidth;			// data per second [MB/s]
	
	double sample_rate;			// samples per second [1/s]
	
	string message;
	
	TimeControl control;
	
	RecordHeader info;
	
}
